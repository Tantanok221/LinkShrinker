<%= form_with(
      model: @short_link,
      url: short_links_path,
      method: :post,
    ) do |form| %>
  <%= form.url_field :target_url, class: "py-2 px-2 pr-24 border-1 border-slate-200 text-muted-foreground rounded-md", placeholder: "Enter your URL here",required: true %>
  <%= form.submit "Shrink!", class: "py-2 px-4 bg-slate-900 text-white rounded-md" %>
<% end %>
